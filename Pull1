class Solution:
    def findMedianSortedArrays(self, nums1: List[int], nums2: List[int]) -> float:
        nums1 = nums1 + nums2
        nums1.sort()
        length = len(nums1)
        mid = length//2
        if length%2 == 0:
            return (nums1[mid-1] + nums1[mid]) / 2
        else:
            return nums1[mid]
